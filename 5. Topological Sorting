from collections import deque

graph = {0:[1,2],1:[3],2:[3],3:[]}
indegree = {i:0 for i in graph}

for u in graph:
    for v in graph[u]:
        indegree[v]+=1

q = deque([i for i in indegree if indegree[i]==0])
order = []

while q:
    u = q.popleft()
    order.append(u)
    for v in graph[u]:
        indegree[v]-=1
        if indegree[v]==0:
            q.append(v)

print(order)
